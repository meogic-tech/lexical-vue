{
  "name": "lexical-vue",
  "type": "module",
  "version": "0.11.1-mod7",
  "description": "An extensible Vue 3 web text-editor based on Lexical.",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/lexical-vue#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/lexical-vue.git"
  },
  "bugs": {
    "url": "https://github.com/wobsoriano/lexical-vue/issues"
  },
  "keywords": [
    "vue",
    "lexical",
    "editor",
    "rich-text"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --onSuccess \"pnpm build:dts\"",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "dev": "tsup --watch --onSuccess \"pnpm build:dts && pnpm --filter playground dev\"",
    "release": "bumpp && npm publish",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint .",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "peerDependencies": {
    "lexical": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "vue": "^3.2.0"
  },
  "dependencies": {
    "@lexical/clipboard": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-clipboard/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/code": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-code/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/dragon": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-dragon/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/hashtag": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-hashtag/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/history": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-history/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/link": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-link/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/list": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-list/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/mark": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-mark/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/markdown": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-markdown/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/overflow": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-overflow/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/plain-text": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-plain-text/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/rich-text": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-rich-text/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/selection": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-selection/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/table": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-table/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/text": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-text/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@lexical/utils": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical-utils/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "lexical": "https://gh2npm-vercel.meogic.com/api/meogic-tech/lexical-for-meogic/packages/lexical/npm?commit=30d10f393d01cadc91b0e12653d9f37c32d8344e",
    "@types/node": "^20.3.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "unplugin-vue": "^4.2.1",
    "vitepress": "1.0.0-beta.1",
    "vue": "^3.3.4",
    "vue-tsc": "^1.6.5"
  }
}
